<script>
var current_bet_appid = 570;

function chooseStream(channel, mode, host) {
    $.ajax({
        url: '/match/stream/host/' + host + '/channel/' + channel + '/mode/' + mode,
        success: function(data) {
            $('#mainstream').html(data);
        }
    });
}

$("#slideToggle").click(function () {
	if ($("#slideToggle").text() == "Show Chatbox") {
		$("#slideToggle").html("Hide Chatbox");
	} else {
		$("#slideToggle").html("Show Chatbox");
	}
    $('.slideTogglebox').slideToggle();
});

$(document).ready(function() {
	$('#betForm').ajaxForm({
		dataType : 'json',
		beforeSerialize : checkBetData,
		success : processJson
	});

	
    $.ajax({
        url          : '/match/index-right/id/' + 12396,
        localCache   : false,
        cacheTTL     : 5,
        cacheKey     : 'd2b_matchRight_' + 12396,
        isCacheValid : function(){
            return true;
        },
        success: function(data) {
            $('#right').html(data);
            resyncTips();
        }
    });

    // quick search regex
    var qsRegex;

    var $container = $('.profile_item_vault').isotope({
    	itemSelector : '.item-blk',
    	layoutMode : 'fitRows',
    	transitionDuration: 0,
        filter: function() {
            return qsRegex ? $(this).text().match( qsRegex ) : true;
          },
          getSortData: {
              name: '.tip',
              category: '[data-category]',
              weight: function( itemElem ) {
                  var weight = $( itemElem ).find('.weight').text();
                  return parseFloat(weight.replace( /[\(\)]/g, ''));
              }
            }
    });

    // use value of search field to filter
    var $quicksearch = $('#quicksearch').keyup( debounce( function() {
      qsRegex = new RegExp( $quicksearch.val(), 'gi' );
      $container.isotope();
    }, 200 ) );

    // filter functions
    var filterFns = {};

    // bind filter on select change
    $('#filters').on('change', function() {
    	// get filter value from option value
    	var filterValue = this.value;
    	// use filterFn if matches value
    	filterValue = filterFns[filterValue] || filterValue;
    	$container.isotope({
    		filter : filterValue
    	});
    });

    // debounce so filtering doesn't happen every millisecond
    function debounce( fn, threshold ) {
      var timeout;
      return function debounced() {
        if ( timeout ) {
          clearTimeout( timeout );
        }
        function delayed() {
          fn();
          timeout = null;
        }
        timeout = setTimeout( delayed, threshold || 100 );
      }
    }
});
