	$(document).pjax('a[data-pjax]', '#load', { fragment: '#load' });
	$(document).on('pjax:beforeSend', function() {
		NProgress.start();
	});
	$(document).on('pjax:complete', function() {
		NProgress.done(true);
		$('.show-arr').hide();
		$('.hide-arr').click(function (e) {
			$(this).parent().parent().parent().find('.s-h').toggle(0);
			$(this).parent().find('.show-arr').show();
			$(this).hide();
	  	});
		$('.show-arr').click(function (e) {
			$(this).parent().parent().parent().find('.s-h').toggle(0);
			$(this).hide();
			$(this).parent().find('.hide-arr').show();
	  	});
	});
    $(document).on('pjax:timeout', function(event) {
      // Prevent default timeout redirection behavior
      event.preventDefault();
    });
});
</script>
</body>

</html>